SELECT 

CHANS.NAME,
CHANS.PASSWORD,
CHANS.TIME_REG,
CHANS.AOP_ENABLED,
CHANS.HOP_ENABLED,
CHANS.SOP_ENABLED,
CHANS.UOP_ENABLED,
CHANS.VOP_ENABLED,
CHANS.ADMIN_ENABLED,
CHANS.OWNER_ENABLED,
BOTS.NICKNAME,
FOUNDER.NICKNAME,
SUCCESSOR.NICKNAME,
CHANS.MLOCK,
CHANS.TOPIC,
CHANS.RESTRICTED,
CHANS.KEEPTOPIC,
CHANS.AUTOVOP,
CHANS.MEMOLEVEL,
CHANS.LEAVEOPS,
CHANS.OPWATCH,
CHANS.URL,
CHANS.TOPICLOCK

FROM CHANS

INNER JOIN BOTS ON CHANS.BOT = BOTS.BOT_ID
INNER JOIN NICKS FOUNDER ON CHANS.FOUNDER = FOUNDER.NICK_ID
LEFT JOIN NICKS SUCCESSOR ON CHANS.SUCCESSOR = SUCCESSOR.NICK_ID;



SELECT * FROM CHANS;
